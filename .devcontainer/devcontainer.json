{ "name": "Stats",
  "dockerFile": "Dockerfile",
  "hostRequirements": {
    "cpus": 1
  },
  "waitFor": "onCreateCommand",
  "updateContentCommand": "echo 'Updating content' && pip install -r requirements.txt",
  "customizations": {
    "codespaces": {
      "openFiles": []
    },
    "vscode": {
      "extensions": [
        "ms-toolsai.jupyter",
        "ms-python.python",
        "ms-python.isort",
        "ms-python.black-formatter",
        "ms-python.flake8",
        "github.copilot"
      ]
    }
  }
}